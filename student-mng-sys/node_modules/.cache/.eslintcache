tiate } from './redux/actions';\r\n\r\n\r\nconst initialState ={\r\n    name: \"\",\r\n    age: \"\",\r\n    gender:\"\",\r\n    contact: \"\",\r\n    email: \"\",\r\n    address: \"\"\r\n}\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n    root: {\r\n        marginTop: 70,\r\n        margin: \"auto\",\r\n        padding: \"15px\",\r\n        maxwidth: \"500px\",\r\n        alignCotent: \"center\",\r\n        \"& > *\": {\r\n            margin: theme.spacing(1),\r\n            width: \"45ch\",\r\n        },\r\n    },\r\n}));\r\n\r\nconst Home = () => {\r\nconst classes = useStyles();\r\nconst [state, setStatus] = useState(initialState);\r\nconst { name, age, gender, contact, email, address} = state;\r\nconst dispatch = useDispatch();\r\nconst {students} = useSelector(state => state.data)\r\n\r\nuseEffect(() => {\r\n    dispatch(getStudentsInitiate());\r\n}, []);\r\n\r\nconst deleteStudent = (id) => {\r\n    if(window.confirm(\"Are you sure that you want to delete ?\")){\r\n        dispatch(deleteStudentInitiate(id));\r\n    }\r\n};\r\n\r\nconst handleInputChange = (e) => {\r\n    let { name,value } = e.target;\r\n    setStatus({ ...state, [name]: value});\r\n};\r\nconst handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    dispatch(addStudentInitiate(state));\r\n    setStatus({name:\"\", age:\"\", gender:\"\", contact:\"\", email:\"\", address:\"\"});\r\n};\r\n    return (\r\n        <MDBContainer fluid>\r\n            <MDBRow>\r\n            <MDBCol md=\"4\">\r\n                    <form onSubmit={handleSubmit} className={classes.root}>\r\n                        <MDBTypography className=\"text-start\" variant=\"h4\">Add Student</MDBTypography>\r\n                        <MDBInput\r\n                        label=\"Full Name\"\r\n                        value={name}\r\n                        name=\"name\"\r\n                        type=\"text\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBInput\r\n                        label=\"Age\"\r\n                        value={age}\r\n                        name=\"age\"\r\n                        type=\"number\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBInput\r\n                        label=\"Gender\"\r\n                        value={gender}\r\n                        name=\"gender\"\r\n                        type=\"text\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBInput\r\n                        label=\"Email\"\r\n                        value={email}\r\n                        name=\"email\"\r\n                        type=\"text\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBInput\r\n                        label=\"Contact\"\r\n                        value={contact}\r\n                        name=\"contact\"\r\n                        type=\"number\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBInput\r\n                        label=\"Address\"\r\n                        value={address}\r\n                        name=\"address\"\r\n                        type=\"text\"\r\n                        onChange={handleInputChange}\r\n                        /><br/>\r\n                        <MDBBtn style={{width: \"100px\"}} color=\"success\" type=\"submit\" align=\"ceter\" >Submit</MDBBtn>\r\n\r\n                    </form>\r\n                </MDBCol>\r\n\r\n                <MDBCol md=\"8\">\r\n                    <MDBTable style={{ marginTop: \"100px\"}} bordered>\r\n                        <MDBTableHead dark>\r\n        <tr>\r\n          <th scope='col'>No</th>\r\n          <th scope='col'>Full Name</th>\r\n          <th scope='col'>Age</th>\r\n          <th scope='col'>Gender</th>\r\n          <th scope='col'>Email</th>\r\n          <th scope='col'>Contact</th>\r\n          <th scope='col'>Address</th>\r\n          <th scope='col'>Action</th>\r\n        </tr>\r\n      </MDBTableHead>\r\n      {students &&\r\n            students.map((item, index) => (\r\n                <MDBTableBody key={index}>\r\n                <tr>\r\n                <th scope='row'>{index+1}</th>\r\n                <td>{item.name}</td>\r\n                <td>{item.age}</td>\r\n                <td>{item.gender}</td>\r\n                <td>{item.email}</td>\r\n                <td>{item.contact}</td>\r\n                <td>{item.address}</td>\r\n                <td>\r\n                    <MDBBtn \r\n                        className= \"m-1\" \r\n                        tag=\"a\" color=\"none\" \r\n                        style={{color : \"#dd4b39\"}} \r\n                        onClick={() => deleteStudent(item.id)}>\r\n\r\n                        <MDBIcon fas icon=\"trash\" size=\"lg\" />\r\n                        \r\n                    </MDBBtn>\r\n                </td>\r\n              </tr>\r\n                </MDBTableBody>\r\n            ))}\r\n     \r\n    </MDBTable>\r\n                </MDBCol>\r\n\r\n            </MDBRow>\r\n        </MDBContainer>\r\n    )\r\n}\r\n\r\nexport default Home","C:\\Users\\Dell\\Desktop\\SPM_Project\\github-basics-KavindyaSathsarani\\student-mng-sys\\src\\redux\\rootReducer.js",[],"C:\\Users\\Dell\\Desktop\\SPM_Project\\github-basics-KavindyaSathsarani\\student-mng-sys\\src\\redux\\actions.js",[],"C:\\Users\\Dell\\Desktop\\SPM_Project\\github-basics-KavindyaSathsarani\\student-mng-sys\\src\\redux\\reducer.js",[],"C:\\Users\\Dell\\Desktop\\SPM_Project\\github-basics-KavindyaSathsarani\\student-mng-sys\\src\\redux\\actionTypes.js",[],"C:\\Users\\Dell\\Desktop\\SPM_Project\\github-basics-KavindyaSathsarani\\student-mng-sys\\src\\firebase.js",[],{"ruleId":"89","severity":1,"message":"90","line":38,"column":4,"nodeType":"91","endLine":